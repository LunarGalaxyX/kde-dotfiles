        {
            "type": "host",
            "key": " PC",
            "keyColor": "white"
        },
        {
            "type": "cpu",
            "key": "│ ├",
            "keyColor": "white"
        },
        {
            "type": "gpu",
            "key": "│ ├󰍛",
            "keyColor": "white"
        },
        {
            "type": "memory",
            "key": "│ ├󰍛",
            "keyColor": "white"
        },
        {
            "type": "disk",
            "key": "└ └",
            "keyColor": "white"
        },
        {
            "type": "custom",
            "format": "\u001b[90m└────────────────────────────────────────────────────┘"
        },
        "break",
        {
            "type": "custom",
            "format": "\u001b[90m┌──────────────────────Software──────────────────────┐"
        },
        {
            "type": "os",
            "key": " OS",
            "keyColor": "white"
        },
