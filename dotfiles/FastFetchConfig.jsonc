            "key": " DE",
            "keyColor": "white"
        },
        {
            "type": "lm",
            "key": "│ ├",
            "keyColor": "white"
        },
        {
            "type": "wm",
            "key": "│ ├",
            "keyColor": "white"
        },
        {
            "type": "wmtheme",
            "key": "│ ├󰉼",
            "keyColor": "white"
        },
        {
            "type": "terminal",
            "key": "└ └",
            "keyColor": "white"
        },
        {
            "type": "custom",
            "format": "\u001b[90m└────────────────────────────────────────────────────┘"
        },
        "break"
    ]
